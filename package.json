{
  "name": "xrouter",
  "version": "0.0.0",
  "description": "Framework-agnostic, isomorphic router. Lightweight and 100% type-safe.",
  "files": [
    "dist"
  ],
  "main": "dist/xrouter.umd.js",
  "unpkg": "dist/xrouter.iife.js",
  "jsdelivr": "dist/xrouter.iife.js",
  "module": "./dist/xrouter.es.js",
  "exports": {
    ".": {
      "import": "./dist/xrouter.es.js",
      "require": "./dist/xrouter.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisshank/xrouter.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisshank/xrouter/discussions"
  },
  "homepage": "https://github.com/chrisshank/xrouter#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "@types/node": "^16.4.3",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.2.3",
    "vite": "^2.2.3",
    "xstate": "^4.22.0"
  }
}
